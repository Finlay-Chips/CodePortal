{
    "manifest_version": 3,
    "name": "Java Assist",
    "version": "1.1",
    "description": "Quickly access Java documentation without leaving your tab",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
  }}
  